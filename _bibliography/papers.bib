@ARTICLE{abellán20232cartesian,
	bibtex_show={true},
	abbr={ArXiv preprint},
	author={{Abellán}, Fernando and {Stern}, Walker H.},
	title = "{2-Cartesian fibrations II: A Grothendieck construction for $\infty$-bicategories}",
	journal = {arXiv e-prints},
	year = 2022,
	month = jan,
	abstract={In this work, we conclude our study of fibred $\infty$-bicategories by providing a Grothendieck construction in this setting. Given a scaled simplicial set $S$ (which need not be fibrant) we construct a 2-categorical version of Lurie's straightening-unstraightening adjunction, thereby furnishing an equivalence between the $\infty$-bicategory of 2-Cartesian fibrations over $S$ and the $\infty$-bicategory of contravariant functors <a>\(S^\{\text o\text p \}\to {\mathbb B\text i\text c\text a\text t}_\infty\)</a> with values in the $\infty$-bicategory of $\infty$-bicategories. We provide a relative nerve construction in the case where the base is a 2-category, and use this to prove a comparison to existing bicategorical Grothendieck constructions. 
	},
	eid = {2201.09589},
	doi = {10.48550/arXiv.2201.09589},
	archivePrefix = {arXiv},
	eprint = {2201.09589},
	primaryClass = {math.AT},
	adsurl = {https://ui.adsabs.harvard.edu/abs/arXiv:2201.09589},
	arxiv = {2201.09589},
}

